    <script>
        document.querySelectorAll('.toggle-btn').forEach(btn => {
            btn.addEventListener('click', () => {
                const targetClass = btn.dataset.target;
                const infoBox = document.querySelector(`.${targetClass}`);
                infoBox.classList.toggle('active');
        
            });
        });
    </script>
